<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - AgroPack</title>
    <!-- Custom fonts for this template-->
    <link href="~/AdminStyles/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">

    <!-- Custom styles for this template-->
    <link href="~/AdminStyles/css/sb-admin-2.min.css" rel="stylesheet">

    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.min1.css" rel="stylesheet" />
    <link href="~/Content/PagedList.css" rel="stylesheet" />
</head>
<body class="bg-gray-200">
@Html.Partial("_Navbar")
<div class="p-4"></div>


<div class="container mt-3">
    @RenderBody()
    <hr class="bg-dark"/>
    <footer>
        <p>&copy; @DateTime.Now.Year - AgroPack</p>
    </footer>
</div>


    <script src="~/Scripts/jquery-3.5.1.min1.js"></script>
    <script src="~/Scripts/popper.min1.js"></script>
    <script src="~/Scripts/bootstrap.min1.js"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
